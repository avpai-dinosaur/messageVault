{
  "manifest_version": 3,
  "key": "",
  "name": "MessageVault - JPay Message Backup Tool",
  "version": "0.2",
  "description": "Automatically extracts and stores JPay messages on your local machine. Preserve communication history with incarcerated loved ones.",
  "permissions": ["tabs", "nativeMessaging", "storage", "identity"],
  "host_permissions": ["https://www.googleapis.com/*"],
  "oauth2": {
    "client_id": "920908808510-o4d294948p6fp5lndacufu7ud553qqlf.apps.googleusercontent.com",
    "scopes": [
       "https://www.googleapis.com/auth/drive"
    ]
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.jpay.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
